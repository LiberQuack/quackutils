{
  "name": "@liberquack/quackutils",
  "version": "1.0.0",
  "description": "Quack utilities",
  "types": "./bundle/types.d.ts",
  "exports": {
    "./*": {
      "module": "./bundle/*.js"
    },
    ".": {
      "module": "./bundle/index.js"
    }
  },
  "browserslist": "Chrome > 80",
  "scripts": {
    "build": "npm run build:js && npm run build:types && mv dist bundle",
    "build:js": "rm -rf bundle dist && tsc -p tsconfig.json",
    "build:types": "rm -rf bundle/types.d.ts dist/types.d.ts && tsc -p tsconfig-declarations.json",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/QuackMartins/quackutils.git"
  },
  "keywords": [
    "quack",
    "utils"
  ],
  "author": "Quack",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/QuackMartins/quackutils/issues"
  },
  "homepage": "https://github.com/QuackMartins/quackutils#readme",
  "dependencies": {
    "bezier-easing": "^2.1.0",
    "haunted": "^5.0.0",
    "idb": "^7.0.2",
    "immer": "^9.0.15",
    "lit": "^2.2.5",
    "react": "^18.2.0",
    "share-buttons": "^1.9.0",
    "superstruct": "^0.16.0"
  },
  "devDependencies": {
    "@parcel/packager-ts": "^2.6.2",
    "@parcel/transformer-inline-string": "^2.6.2",
    "@parcel/transformer-typescript-tsc": "^2.6.2",
    "@parcel/transformer-typescript-types": "^2.6.2",
    "@types/bezier-easing": "^2.1.2",
    "@types/react": "^18.0.15",
    "parcel": "^2.6.2",
    "typescript": "^4.7.2",
    "utility-types": "^3.10.0"
  }
}
